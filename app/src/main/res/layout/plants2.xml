<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.google.android.material.chip.Chip
        android:id="@+id/chip5"
        android:layout_width="145dp"
        android:layout_height="289dp"
        android:layout_marginStart="36dp"
        android:padding="8dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="221dp" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip6"
        android:layout_width="143dp"
        android:layout_height="289dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:padding="8dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="236dp"
        tools:layout_editor_absoluteY="221dp" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip7"
        android:layout_width="142dp"
        android:layout_height="268dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:padding="8dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="535dp" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip8"
        android:layout_width="144dp"
        android:layout_height="268dp"
        app:chipBackgroundColor="@color/design_default_color_primary"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:padding="8dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="236dp"
        tools:layout_editor_absoluteY="535dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="390dp"
        android:layout_height="68dp"
        android:layout_marginTop="4dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:padding="8dp"
        android:drawableLeft="@drawable/baseline_list_24"
        android:drawableRight="@drawable/baseline_shopping_cart_checkout_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="263dp"
        android:layout_height="47dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="sans-serif-black"
        android:padding="8dp"
        android:text="What Kind"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.006"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="72dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="295dp"
        android:layout_height="54dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="sans-serif-black"
        android:padding="8dp"
        android:text="Of Plants You Want"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.008"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="101dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="405dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:background="#2D6200EE"
        android:drawableLeft="@drawable/baseline_zoom_in_24"
        android:hint="Search"
        android:padding="8dp"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="142dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="405dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:hint="    All        Small         Medium      Hanging"
        android:padding="8dp"
        android:textSize="22dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="181dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="102dp"
        android:layout_height="161dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="52dp"
        android:padding="8dp"
        android:contentDescription="plan"
        app:srcCompat="@drawable/plant1"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="307dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="94dp"
        android:layout_height="167dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="52dp"
        android:padding="8dp"
        android:contentDescription="plant"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.904"
        app:layout_constraintStart_toEndOf="@+id/chip5"
        app:layout_constraintTop_toTopOf="@+id/chip6"
        app:srcCompat="@drawable/plant3" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="95dp"
        android:layout_height="141dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="52dp"
        android:padding="8dp"
        android:contentDescription="plantation"
        app:srcCompat="@drawable/images"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="619dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="101dp"
        android:layout_height="144dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="52dp"
        android:padding="8dp"
       android:contentDescription="plants"
        app:srcCompat="@drawable/png"
        tools:layout_editor_absoluteX="257dp"
        tools:layout_editor_absoluteY="618dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>